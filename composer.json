{
    "name": "accionadev/users",
    "description": "Acciona Solutions Plugin - Provides user administration and access list control",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "admad/cakephp-jwt-auth": "^2.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "php-mock/php-mock": "*"
    },
    "autoload": {
        "psr-4": {
            "Acciona\\Users\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acciona\\Users\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
